import java.util.*;

public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    int rows = sc.nextInt();
	    for(int i=rows;i>=1;i--){
	        for(int j=1;j<=i;j++){
	            System.out.print(i+" ");
	        }
	        System.out.println();
	    }
		
	}
}
/*
Example:
Input:
    5
Output:
    5 5 5 5 5 
    4 4 4 4 
    3 3 3 
    2 2 
    1 
*/
