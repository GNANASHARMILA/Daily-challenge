import java.util.*;

public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    int r = sc.nextInt();
	    for(int i=r;i>=1;i--){
	        for(int j=i;j>=1;j--){
	            System.out.print(j+" ");
	        }
	        System.out.println();
	    }
		
	}
}
/*
Example:
Input:
    5
Output:
    5 4 3 2 1 
    4 3 2 1 
    3 2 1 
    2 1 
    1 
*/
