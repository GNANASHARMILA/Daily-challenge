import java.util.*;

public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
	    int n = sc.nextInt();
	    int sum=0;
	    while(n>0){
	        int unitDigit = n%10;
	        if(unitDigit%2 == 1){
	            sum += unitDigit;
	        }
	        n=n/10;
	    }
		System.out.println(sum);
	}
}

/*
Example 1:
Input:
    224
Output:
    0

Example 2:
Input:
    4165
Output:
    6
*/
